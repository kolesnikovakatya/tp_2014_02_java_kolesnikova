<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>Timer</title>
    <script>

        function refresh() {
            location.reload();
        }
    </script>
</head>
<body onload='setInterval(function(){refresh()}, ${refreshPeriod} );'>
    <p>UserID: ${userId}</p>
    <p>Time: ${time}</p>
</body>
</html>
